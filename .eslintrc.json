{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "indent": ["error", 4],
        "global-require": "off",
        "func-names": "off",
        "prefer-arrow-callback": "off",
        "import/no-dynamic-require": "off",
        "no-unused-vars": "off",
        "no-param-reassign": "off"
    }
}
