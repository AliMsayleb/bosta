{
    "token": "use your bot token",

    "plugins": {
        "eval": {
            "timeout": 2,
            "memory": 8,
            "crop": 512,
            "folder": "eval",
            "languages": {
                "haskell": {
                    "command": "runghc",
                    "image": "mchakravarty/ghc-7.10.2:latest",
                    "memory": 8,
                    "timeout": 4
                },
                "javascript": {
                    "command": "node",
                    "image": "google/nodejs:latest",
                    "memory": 8,
                    "timeout": 2
                },
                "perl": {
                    "command": "perl",
                    "image": "python:latest",
                    "memory": 8,
                    "timeout": 2
                },
                "php": {
                    "command": "php",
                    "image": "php:latest",
                    "memory": 8,
                    "timeout": 2
                },
                "python": {
                    "command": "python",
                    "image": "python:latest",
                    "memory": 8,
                    "timeout": 2
                },
                "ruby": {
                    "command": "ruby",
                    "image": "ruby:latest",
                    "memory": 8,
                    "timeout": 2
                },
                "scala": {
                    "command": "scala",
                    "image": "williamyeh/scala:latest",
                    "memory": 64,
                    "timeout": 10
                }
            }
        }
    }
}
